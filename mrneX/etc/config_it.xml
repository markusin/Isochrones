<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <!-- Database network tables -->
	<entry key="tbl.nodes">it_nodes</entry>
    <entry key="tbl.links">it_edges</entry>
    <entry key="tbl.times">it_schedules</entry>
    <entry key="tbl.dateCodes">it_calendar</entry>
    <!-- Database result tables -->
    <entry key="tbl.isoLinks">it_mierwine_rwb</entry>
    <entry key="idx.isoLinks">it_mierwine_rwb_g_idx</entry>
   	<entry key="idx.nodes">it_nodes_g_idx</entry>
   	<!-- Logging table -->
	<entry key="tbl.log_circles">it_mierwine_circles</entry>
	<entry key="tbl.log_nodes">it_mierwine_nodes</entry>
	<entry key="tbl.log_vertex">it_mierwine_red_vertices</entry>
	<!--entry key="tbl.log_vertex">it_mierwine_vertices</entry-->
	<!-- Spatial settings -->
    <entry key="sql.spatial.dim1.el">MDSYS.SDO_DIM_ELEMENT('LONGITUDE',-180,180,0.5)</entry>
    <entry key="sql.spatial.dim2.el">MDSYS.SDO_DIM_ELEMENT('LATITUDE', -90, 90,0.5)</entry>
    <entry key="sql.spatial.srid">3857</entry>
		<!-- precomputation -->
		<entry key="tbl.edge.density">it_edge_density</entry>
		<entry key="tbl.vertex.density">it_density</entry>	
</properties>
