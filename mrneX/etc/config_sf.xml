<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <!-- Database network tables -->
	<entry key="tbl.nodes">sf_nodes</entry>
    <entry key="tbl.links">sf_edges</entry>
    <entry key="tbl.times">sf_schedules</entry>
    <entry key="tbl.dateCodes">sf_calendar</entry>
    <!-- Database result tables -->
    <entry key="tbl.isoLinks">sf_mierwine_rwb</entry>
    <entry key="tbl.resultW">sf_mierwine_rw</entry>
    <!-- Database indices -->
    <entry key="idx.isoLinks">sf_mierwine_rwb_sidx</entry>
    <entry key="idx.resultW">sf_mierwine_rw_sidx</entry>
		<!-- Logging table -->
	<entry key="tbl.log_circles">sf_mierwine_circles</entry>
	<entry key="tbl.log_nodes">sf_mierwine_nodes</entry>
	<entry key="tbl.log_vertex">sf_mierwine_red_vertices</entry>
	<!--entry key="tbl.log_vertex">sf_mierwine_vertices</entry-->
    <!-- Spatial settings -->
    <entry key="sql.spatial.dim1.el">MDSYS.SDO_DIM_ELEMENT('LONGITUDE',-180,180,0.5)</entry>
    <entry key="sql.spatial.dim2.el">MDSYS.SDO_DIM_ELEMENT('LATITUDE', -90, 90,0.5)</entry>
    <entry key="sql.spatial.srid">3857</entry>
		<!-- precomputation -->
		<entry key="tbl.edge.density">sf_edge_density</entry>
		<entry key="tbl.vertex.density">sf_density</entry>	
</properties>
