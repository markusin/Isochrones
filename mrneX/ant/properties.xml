 <tests reservedMemorySize="1024">
  <memoryDurationTest className="isochrones.mrnex.utils.MemoryDurationExperiment">
   <dMax>60</dMax>
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <singleBreakpoint>true</singleBreakpoint>
  </memoryDurationTest>
  <memorySizeTest className="isochrones.mrnex.utils.MemorySizeExperiment">
   <dMax>60</dMax>
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <singleBreakpoint>true</singleBreakpoint>
  </memorySizeTest>
  <confidenceIntervalTest className="isochrones.mrnex.utils.ConfidenceIntervalExperiment">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <initialDuration>1</initialDuration>
   <scale>5</scale>
   <range>15</range>
   <frequency>10</frequency>
   <randomQueryPoint>false </randomQueryPoint>
   <numberOfQueryPoints>10</numberOfQueryPoints>
   <outputDir>./out/</outputDir>
  </confidenceIntervalTest>
  <runtimeDurationTest className="isochrones.mrnex.utils.RuntimeDurationExperiment">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <frequency>5</frequency>
   <outputDir>./out/</outputDir>
   <singleBreakpoint>true</singleBreakpoint>
   <densityLimits>1,5,10,15,20,50,90</densityLimits>
  </runtimeDurationTest>
  <runtimeDurationTestDetailed className="isochrones.mrnex.utils.RuntimeDurationExperimentDetailed">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <frequency>10</frequency>
   <outputDir>./out/</outputDir>
   <singleBreakpoint>true</singleBreakpoint>
  </runtimeDurationTestDetailed>
  <runtimeSizeTest className="isochrones.mrnex.utils.RuntimeSizeExperiment">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <frequency>3</frequency>
   <outputDir>./out/</outputDir>
   <singleBreakpoint>true</singleBreakpoint>
  </runtimeSizeTest>
  <runtimeFixedSizeTest className="isochrones.mrnex.utils.RuntimeFixedSizeExperiment">
   <speed>1.0</speed>
   <dMax>30</dMax>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <frequency>10</frequency>
   <isoSize>2900</isoSize>
   <outputDir>./out/</outputDir>
   <singleBreakpoint>true</singleBreakpoint>
  </runtimeFixedSizeTest>
  <runtimePercentageTest className="isochrones.mrnex.utils.RuntimeExperimentPercentage">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <frequency>10</frequency>
   <outputDir>./out/</outputDir>
  </runtimePercentageTest>
  <runtimeIntervalTest className="isochrones.mrnex.utils.RuntimeIntervalExperiment">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <frequency>10</frequency>
   <outputDir>./out/</outputDir>
  </runtimeIntervalTest>
  <reachabilityTest className="isochrones.mrnex.utils.ReachabilityExperiment">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
   <outputDir>./out/</outputDir>
  </reachabilityTest>
  <memoryDBTest className="isochrones.mrnex.utils.MemoryDurationCountLoadedTuplesExperiment">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
  </memoryDBTest>
  <monitorNetworkLoadingTest className="isochrones.mrnex.launchers.tests.MonitorNetworkLoadingExperiment">
   <speed>1.0</speed>
   <mode>MULTIMODAL</mode>
   <outputWriting>false</outputWriting>
  </monitorNetworkLoadingTest>
  <property name="algorithmClassName" value="${properties.project.test.runtime.algorithm(duration)}" />
 </tests>
</properties>
