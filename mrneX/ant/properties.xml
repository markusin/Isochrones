<?xml version="1.0" encoding="utf-8"?>
<properties>
 <project>
  <algorithm name="MRNEX" className="isochrones.mrnex.algorithm.MRNEX" runtimeDuration="isochrones.mrnex.algorithm.test.RuntimeDurationTest"
   runtimeSize="isochrones.mrnex.algorithm.test.RuntimeSizeTest">
   <classNames main="isochrones.mrnex.algorithm.MRNEX">
    <runtimeTest>
     <duration>isochrones.mrnex.algorithm.test.RuntimeDurationTest</duration>
     <durationDetailed>isochrones.mrnex.algorithm.test.RuntimeDurationTestDetailed
     </durationDetailed>
     <size>isochrones.mrnex.algorithm.test.RuntimeSizeTest</size>
    </runtimeTest>
    <memoryTest>
     <duration>isochrones.mrnex.algorithm.test.MemoryDurationTest</duration>
     <size>isochrones.mrnex.algorithm.test.MemorySizeTest</size>
    </memoryTest>
   </classNames>
  </algorithm>
  <srcDir>src/isochrones</srcDir>
  <classDir>classes</classDir>
  <libDir>lib</libDir>
  <libextDir>libext</libextDir>
  <database indexCheck="true">postgresql</database>
 </project>
