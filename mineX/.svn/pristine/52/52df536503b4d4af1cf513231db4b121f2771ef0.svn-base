		<property name="name" value="${properties.dataSets.BZ(name)}" />
		<property name="lOI" value="${properties.dataSets.BZ.parameters.poi.central.locationOfInterests}" />
		<property name="targetTime" value="${properties.dataSets.BZ.parameters.tArrival}" />
		<property name="dMax" value="${properties.dataSets.BZ.parameters.dMax}" />
		<property name="queryPointsTable" value="${properties.dataSets.BZ.queryPointsTable}" />

		<!-- overide only in some special cases -->
		<property name="mode" value="${properties.tests.confidenceIntervalTest.mode}" />

		<echo message="Starting the confidence interval tests for ${name}" />
		<mkdir dir="${properties.tests.confidenceIntervalTest.outputDir}" />

		<java classname="${properties.tests.confidenceIntervalTest(className)}" classpathref="path.libraries" fork="true">
			<arg value="algorithm=${properties.project.name}" />
			<arg value="dataset=${dataset}" />
			<arg value="lOI=${lOI}" />
			<arg value="dMax=${dMax}" />
			<arg value="targetTime=${targetTime}" />
			<arg value="speed=${properties.tests.confidenceIntervalTest.speed}" />
			<arg value="mode=${mode}" />
			<arg value="outputWriting=${properties.tests.confidenceIntervalTest.outputWriting}" />
			<arg value="optimization=${properties.tests.confidenceIntervalTest.optimization}" />
			<arg value="mode=${mode}" />
			<arg value="frequency=${properties.tests.confidenceIntervalTest.frequency}" />
			<arg value="outputDir=${properties.tests.confidenceIntervalTest.outputDir}/" />
